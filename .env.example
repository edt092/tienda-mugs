# Configuración de Payphone
# Obtén estos valores desde tu cuenta de Payphone
# https://pay.payphonetodoesposible.com

# Token de autenticación de Payphone (debe incluir "Bearer ")
PAYPHONE_TOKEN="Bearer eyJhbGciOiJI..."

# URL base de la API de Payphone
PAYPHONE_API_URL="https://pay.payphonetodoesposible.com"

# Token público de Payphone para el frontend (si aplica)
NEXT_PUBLIC_PAYPHONE_TOKEN="tu_token_publico_aqui"

# App ID de Payphone para cargar el script del botón
NEXT_PUBLIC_PAYPHONE_APP_ID="tu_app_id_aqui"

# Configuración de Formspree
# Crea un formulario en https://formspree.io y obtén tu ID

# ID del formulario de Formspree (formato: xxxxxxxxxxx)
NEXT_PUBLIC_FORMSPREE_ID="tu_formspree_id_aqui"

# Nota: Recuerda crear un archivo .env.local con tus valores reales
# y NUNCA subir este archivo al repositorio
